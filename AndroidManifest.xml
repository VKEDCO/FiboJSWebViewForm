            android:name="org.vkedco.mobicom.web_view_act_02.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.vkedco.mobicom.web_view_act_02.FiboFormWebViewer"
